<!DOCTYPE html>
<html>
  <head>
    <title>TasksManager</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link href="https://fonts.googleapis.com/css?family=Muli:300,400,700,900" rel="stylesheet">
    <%=  stylesheet_link_tag 'fontawesome/css/all_fontawesome.min' %>
    <%=  stylesheet_link_tag 'landing/bootstrap.min' %>
    <%=  stylesheet_link_tag 'landing/fonts/icomoon/style' %>
    <%=  stylesheet_link_tag 'landing/fonts/flaticon/font/flaticon' %>
    <%=  stylesheet_link_tag 'landing/owl.carousel.min' %>
    <%=  stylesheet_link_tag 'landing/owl.theme.default.min' %>
    <%=  stylesheet_link_tag 'landing/jquery-ui.css' %>
    <%=  stylesheet_link_tag 'landing/jquery.fancybox.min' %>
    <%=  stylesheet_link_tag 'landing/bootstrap-datepicker' %>
    <%=  stylesheet_link_tag 'landing/aos' %>
    <%=  stylesheet_link_tag 'landing/style' %>
  </head>

  <body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">

<!--  <p class="navbar-text pull-right">-->
    <%# if user_signed_in? %>
<!--      Logged in as <strong><%#= current_user.email %></strong>.-->
<!--      <%#= link_to 'Edit profile', edit_user_registration_path, :class => 'navbar-link' %> |-->
      <%#= link_to "Logout", destroy_user_session_path, method: :delete, :class => 'navbar-link'  %>
    <%# else %>
<!--      <%#= link_to "Sign up", new_user_registration_path, :class => 'navbar-link'  %> |-->
      <%#= link_to "Login", new_user_session_path, :class => 'navbar-link'  %>
    <%# end %>
<!--  </p>-->


  <%= yield %>

    <%= javascript_include_tag 'landing/jquery-migrate-3.0.1.min'  %>
    <%= javascript_include_tag 'landing/jquery-ui'  %>
    <%= javascript_include_tag 'landing/owl.carousel.min'  %>
    <%= javascript_include_tag 'landing/jquery.stellar.min'  %>
    <%= javascript_include_tag 'landing/jquery.countdown.min'  %>
    <%= javascript_include_tag 'landing/bootstrap-datepicker.min'  %>
    <%= javascript_include_tag 'landing/jquery.easing.1.3'  %>
    <%= javascript_include_tag 'landing/aos'  %>
    <%= javascript_include_tag 'landing/jquery.fancybox.min'  %>
    <%= javascript_include_tag 'landing/jquery.sticky'  %>
    <%= javascript_include_tag 'landing/main'  %>
  </body>
</html>
